// Global default params, used in configs
workDir = "analysis"

params {

  // Boilerplate options
  help = false

  // cache option makes it a bit easier to set conda or singularity cacheDir
  cache = ''
}

includeConfig 'config/base.config'

process {
    withName: fasttree {
        cpus = 3
        memory = 4.GB
    }
}